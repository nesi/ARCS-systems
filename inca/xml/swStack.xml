<?xml version = "1.0" encoding = "UTF-8"?>
<stack>
    <id>Software and Services</id>
    <category>
        <id>Math Libraries</id>
        <package>
            <id>atlas</id>
            <version>any</version>
            <versionRE>^.*</versionRE>
            <tests>
                <version>
			<id>atlas_version</id>
		</version>
            </tests>
        </package>
    </category>
    <category>
        <id>Compilers</id>
        <package>
            <id>gcc</id>
            <version>any</version>
            <versionRE>(^.*)</versionRE>
            <tests>
                <version>
			<id>gcc_version</id>
		</version>
            </tests>
        </package>
        <package>
            <id>java</id>
            <tests>
                <unitalias>
			<id>java_hello_world</id>
		</unitalias>
            </tests>
        </package>
    </category>
