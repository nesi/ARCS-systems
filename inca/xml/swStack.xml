        <id>Grid Services</id>
        <package>
            <id>openssl</id>
            <version>0.9.*</version>
            <versionRE>^0\.9(\.*|)</versionRE>
            <tests>
                <version>
			<id>openssl_version</id>
		</version>
            </tests>
        </package>
        <package>
            <id>openssh</id>
            <version>any</version>
            <versionRE>^.*</versionRE>
            <tests>
